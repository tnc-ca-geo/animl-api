{
    "name": "animl-serverless",
    "version": "1.0.0",
    "description": "Lambda-based graphQL API for camera trap data managment platform",
    "main": "src/handler.js",
    "scripts": {
        "start": "serverless offline",
        "deploy-dev": "sls deploy --stage dev",
        "deploy-prod": "sls deploy --stage prod",
        "seed-db": "AWS_PROFILE=animl REGION=us-west-2 node ./src/scripts/seedDb.js",
        "seed-db-dev": "STAGE=dev npm run seed-db",
        "seed-db-prod": "STAGE=prod npm run seed-db",
        "export-db": "AWS_PROFILE=animl REGION=us-west-2 node ./src/scripts/exportDb.js",
        "export-db-dev": "STAGE=dev npm run export-db",
        "export-db-prod": "STAGE=prod npm run export-db",
        "update-docs": "AWS_PROFILE=animl REGION=us-west-2 node ./src/scripts/updateDocuments.js",
        "update-docs-dev": "STAGE=dev npm run update-docs",
        "update-docs-prod": "STAGE=prod npm run update-docs",
        "test": "tape test/**.test.js",
        "lint": "eslint src/**/*.js test/**.js"
    },
    "author": "Nathaniel Rindlaub",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.181.0",
        "@aws-sdk/client-sagemaker-runtime": "^3.182.0",
        "@aws-sdk/client-secrets-manager": "^3.181.0",
        "@aws-sdk/client-ses": "^3.181.0",
        "@aws-sdk/client-sqs": "^3.181.0",
        "@aws-sdk/client-ssm": "^3.181.0",
        "@aws-sdk/lib-storage": "^3.182.0",
        "@aws-sdk/s3-request-presigner": "^3.182.0",
        "apollo-server-errors": "^3.3.1",
        "apollo-server-lambda": "^3.10.3",
        "app-root-path": "^3.0.0",
        "async-retry": "^1.3.3",
        "crypto": "^1.0.1",
        "csv-parse": "^5.3.0",
        "csv-stringify": "^6.2.0",
        "graphql": "^16.6.0",
        "graphql-request": "^3.4.0",
        "graphql-type-json": "^0.3.2",
        "jwt-simple": "^0.5.6",
        "lodash": "^4.17.20",
        "luxon": "^3.2.1",
        "mongo-cursor-pagination": "^8.1.2",
        "mongodb-query-parser": "^2.4.3",
        "mongoose": "^5.10.3",
        "prompt": "^1.2.0",
        "stream-transform": "^3.2.0",
        "superagent": "^6.1.0"
    },
    "devDependencies": {
        "@mapbox/mock-aws-sdk-js": "^1.0.0",
        "eslint": "^8.19.0",
        "eslint-plugin-node": "^11.1.0",
        "nock": "^13.2.8",
        "serverless": "^3.20.0",
        "serverless-dotenv-plugin": "^4.0.1",
        "serverless-offline": "^12.0.4",
        "tape": "^5.5.3"
    }
}
