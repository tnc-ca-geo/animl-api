{
  "name": "animl-serverless",
  "version": "1.0.0",
  "description": "Lambda-based graphQL API for camera trap data managment platform",
  "main": "src/handler.js",
  "scripts": {
    "start": "serverless offline --useSeparateProcesses --env dev",
    "deploy-dev": "sls deploy --stage dev",
    "deploy-prod": "sls deploy --stage prod",
    "seed-db": "AWS_PROFILE=animl REGION=us-west-2 node ./src/scripts/seedDb.js",
    "seed-db-dev": "STAGE=dev npm run seed-db",
    "seed-db-prod": "STAGE=prod npm run seed-db",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Nathaniel Rindlaub",
  "license": "ISC",
  "dependencies": {
    "apollo-server-errors": "^2.5.0",
    "aws-sdk": "^2.834.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-type-json": "^0.3.2",
    "graphql-yoga": "^1.18.3",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "mongo-cursor-pagination": "^7.3.1",
    "mongoose": "^5.10.3",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-offline": "^6.7.0",
    "serverless-offline-sqs": "^4.1.1",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "serverless": "^2.46.0"
  }
}
